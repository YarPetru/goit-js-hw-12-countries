<div class="render">
    <div class="country-info">
        <h2>{{name}}</h2>
        <h3>Capital: {{capital}}</h3>
        <h3>Population: {{population}}</h3>
        <h3>Languages:</h3>
        <ul>
            {{#each languages}}
                <li>{{this.name}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="country-flag">
        <img src="{{flag}}" alt="{{name}}">
    </div>
</div>
